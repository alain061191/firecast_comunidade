<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="br.gov.sc.cbm.e193comunitario.ui.DefinirCidadeActivity" >

    <TextView
        android:id="@+id/tv_escolher_cidade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tv_cidade"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Spinner
        android:id="@+id/sp_cidade"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />


    <ImageButton
        android:id="@+id/img_btn_ok"
        android:contentDescription="@string/btn_salvar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:src="@drawable/save_ok" />

</LinearLayout>
